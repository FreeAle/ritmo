      }, {
        key: "addToCartProduct",
        value: function addToCartProduct() {
          var _this11 = this;

          var total = 0; //this.loading.autoHide(500);
          //console.log(this.selectAttributes);

          if (this.product.type == 'variable') {
            this.shared.addToCart(this.product, this.selectedVariation, this.quantity, this.selectAttributes);
            this.navCtrl.pop(); //this.navCtrl.push(CartPage);
          }

          if (this.product.type == 'simple') {
            this.shared.addToCart(this.product, null, this.quantity, null);
            this.navCtrl.pop(); //this.navCtrl.push(CartPage);
          }

          if (this.product.type == 'grouped') {
            var _iterator12 = _createForOfIteratorHelper(this.groupProducts),
                _step12;

            try {
              for (_iterator12.s(); !(_step12 = _iterator12.n()).done;) {
                var a = _step12.value;
                total = total + a.quantity;
              }
            } catch (err) {
              _iterator12.e(err);
            } finally {
              _iterator12.f();
            }

            if (total == 0) this.shared.translateString("Please Add Quantity").then(function (res) {
              _this11.shared.showAlert(res);
            });else {
              var _iterator13 = _createForOfIteratorHelper(this.groupProducts),
                  _step13;

              try {
                for (_iterator13.s(); !(_step13 = _iterator13.n()).done;) {
                  var value = _step13.value;
                  if (value.quantity != 0) this.shared.addToCart(value, null, value.quantity, null);
                }
              } catch (err) {
                _iterator13.e(err);
              } finally {
                _iterator13.f();
              }
            }

            if (total != 0) {
              this.navCtrl.pop(); //this.navCtrl.push(CartPage);
            }
          }
        } //===============================================================================================================================

      }, {
        key: "selectVariation",
        value: function selectVariation() {
          console.log(this.tempAllVariableAttributes);
          var pId = null;

          var _iterator14 = _createForOfIteratorHelper(this.tempAllVariableAttributes),
              _step14;

          try {
            for (_iterator14.s(); !(_step14 = _iterator14.n()).done;) {
              var i = _step14.value;

              if (i.select == true) {
                pId = i.id;
                break;
              }
            }
          } catch (err) {
            _iterator14.e(err);
          } finally {
            _iterator14.f();
          }

          var _iterator15 = _createForOfIteratorHelper(this.variations),
              _step15;

          try {
            for (_iterator15.s(); !(_step15 = _iterator15.n()).done;) {
              var _i = _step15.value;

