        }
      }, {
        key: "availableOption",
        value: function availableOption(name, val) {
          if (this.selectAttributes.length == 0) return true;

          var _iterator7 = _createForOfIteratorHelper(this.tempAllVariableAttributes),
              _step7;

          try {
            for (_iterator7.s(); !(_step7 = _iterator7.n()).done;) {
              var value = _step7.value;

              if (value.select == true) {
                if (value[name] == 'any') return true;
                if (this.removeDiacritics(value[name]) == this.removeDiacritics(val)) return true;
              }
            }
          } catch (err) {
            _iterator7.e(err);
          } finally {
            _iterator7.f();
          }
        } //checking avalability of option in all variations

      }, {
        key: "sortAttributes",
        value: function sortAttributes() {
          var _this10 = this;

          this.tempAllVariableAttributes = JSON.parse(JSON.stringify(this.allVariableAttributes));
          var count = 0; //console.log(this.tempAllVariableAttributes);
          //console.log(this.selectAttributes);

          var _iterator8 = _createForOfIteratorHelper(this.selectAttributes),
              _step8;

          try {
            var _loop = function _loop() {
              var x = _step8.value;

              var _iterator9 = _createForOfIteratorHelper(_this10.tempAllVariableAttributes),
                  _step9;

              try {
                for (_iterator9.s(); !(_step9 = _iterator9.n()).done;) {
                  var y = _step9.value;
                  //converting object into array
                  var keyOfY = "";
                  _this = _this10;
                  Object.keys(y).forEach(function (yKeys) {
                    var v1 = _this.removeDiacritics(x.key);

                    var v2 = _this.removeDiacritics(yKeys);

                    if (v1 == v2) {
                      keyOfY = yKeys; //console.log(x.key + " found  " + yKeys);
                    }
                  });

                  if (_this10.removeDiacritics(y[keyOfY]) == _this10.removeDiacritics(x.value) || y[keyOfY] == 'any') {
                    if (count == 0) {
                      y.select = true;
                    } else {
                      if (y.select == true) y.select = true;else y.select = false;
                    } //console.log(y[x.key] + "   ---  " + x.value);

                  } else y.select = false;
                }
              } catch (err) {
                _iterator9.e(err);
              } finally {
                _iterator9.f();
              }

              count++;
            };

            for (_iterator8.s(); !(_step8 = _iterator8.n()).done;) {
              var _this;

              _loop();
            } //console.log("attributes after select");

          } catch (err) {
            _iterator8.e(err);
          } finally {
            _iterator8.f();
          }

          console.log(this.tempAllVariableAttributes);
        } // reset attributes and selection

      }, {
