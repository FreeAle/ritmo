          }
        } //===============================================================================================================================

      }, {
        key: "initializeAllVariationAttributes",
        value: function initializeAllVariationAttributes(p) {
          var ob = {};
          ob.id = p.id;
          ob.select = false;

          var _iterator3 = _createForOfIteratorHelper(this.attributes),
              _step3;

          try {
            for (_iterator3.s(); !(_step3 = _iterator3.n()).done;) {
              var val = _step3.value;
              if (val.variation == false) continue;
