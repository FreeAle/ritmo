                _this8.variations.push(d);

                _this8.initializeAllVariationAttributes(d); //console.log(count);


                if (count == _this8.product.variations.length) {
                  _this8.loaderProductVariations = false;

                  _this8.product.default_attributes.forEach(function (e, ind) {
                    _this8.fillAttributes(e.option, e.name, ind);
                  });
                }
              });
            }
          } catch (err) {
