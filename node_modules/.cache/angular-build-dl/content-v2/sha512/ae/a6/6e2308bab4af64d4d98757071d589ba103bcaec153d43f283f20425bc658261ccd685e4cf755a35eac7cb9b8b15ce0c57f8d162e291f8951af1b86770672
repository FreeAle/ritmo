                for (_iterator4.s(); !(_step4 = _iterator4.n()).done;) {
                  var v2 = _step4.value;
                  if (this.removeDiacritics(val.name) == this.removeDiacritics(v2.name)) ob[val.name] = this.removeDiacritics(v2.option);
                }
              } catch (err) {
                _iterator4.e(err);
              } finally {
                _iterator4.f();
              }
            }
          } catch (err) {
            _iterator3.e(err);
          } finally {
            _iterator3.f();
          }

          this.allVariableAttributes.push(ob);
          if (this.allVariableAttributes.length == this.product.variations.length) this.sortAllVariationAttributes();
          console.log(this.allVariableAttributes);
        }
      }, {
