                this.selectedVariation = _i;
                break;
              }
            }
          } catch (err) {
            _iterator15.e(err);
          } finally {
            _iterator15.f();
          }

          if (this.selectAttributes != null) this.updateProductDetail(this.selectedVariation);
        } //===============================================================================================================================

      }, {
        key: "updateProductDetail",
        value: function updateProductDetail(p) {
          if (p.images) {
            this.product.images = p.images;
          } else {
            var pImges = [];
            pImges.push(p.image); //console.log(pImges);

            this.product.images = this.removeDuplication(pImges.concat(this.product.images));
          }
        }
      }, {
        key: "removeDuplication",
        value: function removeDuplication(array) {
          var a = array.concat(); // console.log(a);

          for (var i = 0; i < a.length; ++i) {
            for (var j = i + 1; j < a.length; ++j) {
              if (a[i].src === a[j].src) a.splice(j--, 1);
            }
          } //console.log(a);


          return a;
        } //===============================================================================================================================

      }, {
        key: "checkProductNew",
        value: function checkProductNew() {
          var pDate = new Date(this.product.date_created);
          var date = pDate.getTime() + this.config.newProductDuration * 86400000;
          var todayDate = new Date().getTime();
          if (date > todayDate) return true;else return false;
        }
      }, {
        key: "quantityChange",
        value: function quantityChange() {
          var _this12 = this;

          console.log("quantity updated");

          if (this.product.stock_quantity == null) {
            console.log("quantity is unlimited");
          } else if (this.product.stock_quantity > this.quantity) {
            console.log("quantity is less than stock quantity");
          } else if (this.product.stock_quantity < this.quantity) {
            this.shared.translateString("Product Quantity is Limited!").then(function (res) {
              if (_this12.product.stock_quantity == null) _this12.quantity = 1;else _this12.quantity = parseInt(_this12.product.stock_quantity);

              _this12.shared.showAlert(res);
            });
          }

          if (this.quantity == null || this.quantity == 0 || this.quantity < 0) {
            this.quantity = 1;
          }

          this.applicationRef.tick();
        } //===============================================================================================================================

      }, {
        key: "showProductDetail",
